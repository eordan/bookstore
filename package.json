{
  "name": "react-typescript-jest-boilerplate",
  "version": "1.4.0",
  "description": "Boilerplate for React app with Typescript and Jest lib",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --hot --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "format": "npx prettier --write .",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:fix": "npm run lint:code:fix && npm run lint:style:fix",
    "lint:code": "eslint src/**/*.{ts,tsx} --quiet",
    "lint:code:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:style": "stylelint src/**/*.scss",
    "lint:style:fix": "stylelint src/**/*.scss --fix",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkoziol/react-typescript-jest-boilerplate.git"
  },
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "jest",
    "boilerplate",
    "rkoziol"
  ],
  "author": "rkoziol",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rkoziol/react-typescript-jest-boilerplate/issues"
  },
  "homepage": "https://github.com/rkoziol/react-typescript-jest-boilerplate#readme",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/iso-3166-1-alpha-2": "^1.0.0",
    "@types/jest": "29.4.0",
    "@types/node": "^20.4.8",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-router-bootstrap": "^0.26.2",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "css-loader": "6.7.3",
    "eslint": "8.33.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest-environment-jsdom": "^29.6.3",
    "mini-css-extract-plugin": "2.7.2",
    "prettier": "2.8.3",
    "react-toastify": "^9.1.3",
    "sass": "1.58.0",
    "sass-loader": "13.2.0",
    "source-map-loader": "4.0.1",
    "style-loader": "3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "@commercetools/importapi-sdk": "^4.6.2",
    "@commercetools/platform-sdk": "^4.11.0",
    "@commercetools/sdk-client-v2": "^2.2.0",
    "@testing-library/jest-dom": "^6.1.1",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.1",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb": "^19.0.4",
    "exports-loader": "^4.0.0",
    "iso-3166-1-alpha-2": "^1.0.1",
    "jest": "29.4.1",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "node-fetch": "^2.6.12",
    "postcss-loader": "^7.3.3",
    "react": "18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "6.8.0",
    "react-test-renderer": "18.2.0"
  }
}
